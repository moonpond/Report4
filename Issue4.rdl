  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>ContosoRetailDW</DataSourceName>
        <CommandText>select      m.prodkey, m.product, m.category, SalesAmount, NetSoldQuantity,rn 
from        [dbo].[DimProductCategory] as c left join
            (select ROW_NUMBER() over (partition by category order by SalesAmount desc) as rn, 
			prodkey, product, category, SalesAmount, [NetSoldQuantity]
             from [dbo].[vTopSaleProduct]) as m
			 on c.ProductCategoryName = m.category
where rn between 1 and 3;</CommandText>
      </Query>
      <Fields>
        <Field Name="product">
          <DataField>product</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="prodkey">
          <DataField>prodkey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="category">
          <DataField>category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SalesAmount">
          <DataField>SalesAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="NetSoldQuantity">
          <DataField>NetSoldQuantity</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="rn">
          <DataField>rn</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
